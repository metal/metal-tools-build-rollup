{
  "name": "metal-tools-build-rollup",
  "version": "2.0.5",
  "description": "Tool that can be used to build metal ES6 js files with Rollup",
  "license": "BSD",
  "repository": "metal/metal-tools-build-rollup",
  "author": {
    "name": "Maira Bello",
    "email": "maira.araujo@liferay.com"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "files": [
